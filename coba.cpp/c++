#include <iostream>
using namespace std;

int main() {
    string nama = "ikhsan";
    int umur = 19;
    float tinggi = 170;
    bool aktif = true;

    cout << "Nama: " << nama << endl;
    cout << "Umur: " << umur << endl;
    cout << "Tinggi: " << tinggi << endl;
    cout << "Status aktif: " << (aktif ? "true" : "false") << endl;
    return 0;
}